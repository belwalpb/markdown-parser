{
    "name": "md-json-parser",
    "version": "1.0.3",
    "description": "Markdown To JSON Parsing Library With Components Support.",
    "exports": {
        ".": {
            "import": "./lib/index.mjs",
            "default": "./lib/index.mjs",
            "node": "./lib/index.mjs",
            "require": null
        }
    },
    "module": "./lib/index.mjs",
    "main": "./lib/index.mjs",
    "types": "./lib/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "rimraf lib && unbuild",
        "lint": "eslint \"src/**/*.{js,ts}\"",
        "lint:fix": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
        "format": "prettier --write src/**/*.{ts,tsx}",
        "pack": "npm run build && npm pack"
    },
    "author": "Priyanshu Belwal",
    "license": "Apache-2.0",
    "dependencies": {
        "defu": "^6.1.2",
        "mdast-util-to-hast": "^13.0.2",
        "rehype-external-links": "^3.0.0",
        "rehype-raw": "^7.0.0",
        "rehype-sanitize": "^6.0.0",
        "rehype-slug": "^6.0.0",
        "rehype-sort-attribute-values": "^5.0.0",
        "rehype-sort-attributes": "^5.0.0",
        "remark-emoji": "^4.0.0",
        "remark-gfm": "^4.0.0",
        "remark-mdc": "^2.0.0",
        "remark-parse": "^11.0.0",
        "remark-rehype": "^11.0.0",
        "remark-squeeze-paragraphs": "^6.0.0",
        "unified": "^11.0.3",
        "unist-builder": "^4.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "eslint": "^8.52.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-n": "^16.2.0",
        "eslint-plugin-promise": "^6.1.1",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "typescript": "^5.2.2",
        "unbuild": "^2.0.0"
    },
    "lint-staged": {
        "src/**/*.{js,ts}": [
            "prettier --write",
            "eslint \"src/**/*.{js,ts}\" --quiet --fix"
        ]
    },
    "keywords": [
        "markdown-parser",
        "markdown-json-converter",
        "md-parser",
        "md-json-converter",
        "markdown-parser-unified",
        "markdown-parser-remark",
        "markdown-parser-rehype"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/belwalpb/md-json-parser.git"
    },
    "bugs": {
        "url": "https://github.com/belwalpb/md-json-parser/issues"
    },
    "homepage": "https://github.com/belwalpb/md-json-parser.git/#readme"
}
